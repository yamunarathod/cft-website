    <script type="module">
        // Import the Firebase SDK and required modules
        import { initializeApp } from "https://www.gstatic.com/firebasejs/9.23.0/firebase-app.js";
        import { getFirestore, collection, addDoc, serverTimestamp } from "https://www.gstatic.com/firebasejs/9.23.0/firebase-firestore.js";
      
        // Firebase configuration
        const firebaseConfig = {
          apiKey: "AIzaSyDk_vIJt6ecJlQRHGgNagM_ZpHgH1i5z7c",
          authDomain: "cft-web.firebaseapp.com",
          projectId: "cft-web",
          storageBucket: "cft-web.appspot.com",
          messagingSenderId: "800090693406",
          appId: "1:800090693406:web:7e8ea9d1ef2021e600330f",
          measurementId: "G-YC3Q16J1V8"
        };
      
        // Initialize Firebase
        const app = initializeApp(firebaseConfig);
        const db = getFirestore(app);
      
        // Add event listener for form submission
        document
          .getElementById("wf-form-Contact-form")
          .addEventListener("submit", async function (event) {
            event.preventDefault();
      
            // Collect form data
            const formData = {
              name: document.getElementById("Form-Name").value,
              email: document.getElementById("Form-Email").value,
              phone: document.getElementById("Form-Phone").value,
              subject: document.getElementById("Form-Subject").value,
              choice: document.getElementById("Form-Select").value,
              message: document.getElementById("Form-Message").value,
              subscribe: document.getElementById("Subscribe-to-newsletter").checked,
              timestamp: serverTimestamp(),  // Add a timestamp field
            };
      
            try {
              // Add a new document in collection "contactFormSubmissions"
              await addDoc(collection(db, "contactFormSubmissions"), formData);
              alert("Your message has been sent successfully!");
            } catch (error) {
              console.error("Error writing document to Firestore:", error);
              alert("Failed to send your message. Please try again.");
            }
          });
      </script>
      
